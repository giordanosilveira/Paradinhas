package aula10;


public class Exercicio1 {
    
	
	public static <T extends Comparable<T>> void ordena(T[] v) {
		int min, aux;
		for (int i = 0; i < v.length; i++) {
			min = i;
			for (int j = (i + 1); j < v.length; j++) {
				//Comparar se Ã© menor
				//if (v[j] < v[min]) {
					
				//}
			}
			if (i != min) {
				aux = v[i];
				v[i] = v[min];
				v[min] = aux;
			}
		}
	}
	
	
	public static <T extends Comparable<T>> T minino() {
		
	}
	
